# Frontend production environment for Render
# Copy this file to .env and fill in your actual values

# ===== API CONFIGURATION =====
VITE_BACKEND_URL=https://meddirect-backend.onrender.com

# ===== GOOGLE SERVICES =====
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# ===== PAYMENT GATEWAYS (PUBLIC KEYS) =====
# Stripe (Global)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key

# Razorpay (India)
VITE_RAZORPAY_KEY_ID=rzp_live_your_key_id

# ===== EXTERNAL APIs =====
VITE_OPENFDA_API_KEY=your_openfda_api_key

# ===== APP CONFIGURATION =====
VITE_APP_NAME=MedDirect
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# ===== FEATURES FLAGS =====
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_LOCATION_SERVICES=true
VITE_ENABLE_PAYMENTS=true

# ===== ANALYTICS (Optional) =====
VITE_GOOGLE_ANALYTICS_ID=your_ga_tracking_id